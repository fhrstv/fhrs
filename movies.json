import requests
import json

api_url = 'https://vidsrc.xyz/movies/latest/page-'
max_pages = 1543

movies = []

for page in range(1, max_pages + 1):
    url = f'{api_url}{page}.json'
    response = requests.get(url)
    data = response.json()
    movies.extend(data['result'])

with open('movies.json', 'w') as f:
    json.dump(movies, f)